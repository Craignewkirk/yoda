monitor:
    require:
     - rp/logger
     - rp/static
     - rp/memcached
     - kcmerrill/automagicproxy
    remove: true
    image: docker-registry.returnpath.net:443/assurance/emt_web:version_435
    p:
     - "1200:80"
    h: docker-w3clidev.returnpath.net
    v:
     - $PWD/src:/var/www/html/
     - "$PWD/src/docker/ansible/oracle:/container/repos.oracle"
     - "$PWD/src/docker:/tmp/docker"
     - /tmp/syslogdev:/dev
     - /etc/localtime:/etc/localtime:ro
    link:
     - memcached:memcached
    entrypoint: /container/entrypoint.sh
    setup:
     - git clone -q ssh://gerrit/emt_web src && cd src && git submodule update -q --init --recursive && sh ./devSetup.sh && ./composer.phar install
    e:
     - server_env=dev
     - env=1
     - is_docker=1
     - site_name=emt
