syslogweb:
    image: docker-registry.returnpath.net:443/logger/syslog-web
    remove: true
    hostname: docker-dev.syslog-web
    e:
     - RP_ENV=dev
    v:
     - /etc/localtime:/etc/localtime:ro
     - /tmp/syslogdev:/dev
    env:
      prod:
       e:
        - RP_ENV=prod
      test:
       e:
        - RP_ENV=test
      ci:
       e:
        - RP_ENV=ci

